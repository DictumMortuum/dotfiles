[
  {
    "key": "ctrl+insert",
    "command": "editor.action.selectionClipboardPaste",
    "when": "config.editor.selectionClipboard && editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+insert",
    "command": "-editor.action.selectionClipboardPaste",
    "when": "config.editor.selectionClipboard && editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+insert",
    "command": "editor.action.clipboardPasteAction",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+v",
    "command": "-editor.action.clipboardPasteAction",
    "when": "textInputFocus && !editorReadonly"
  }
]
