"*":
  core:
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "CVS"
    ]
    projectHome: "$HOME/Code"
    themes: [
      "atom-material-ui"
      "atom-material-syntax-dark"
    ]
  editor:
    fontFamily: "Droid Sans Mono"
    invisibles: {}
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
  "exception-reporting":
    userId: "1f590c8c-b4c8-6163-1c46-64df5999717e"
  "file-icons":
    coloured: false
    onChanges: true
  "linter-eslint":
    disableWhenNoEslintConfig: false
    eslintrcPath: "$HOME/.eslintrc.json"
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
    squashDirectoryNames: true
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
