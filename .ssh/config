Host *
  ControlMaster auto
  # man ssh_config defines the %C format as 'a hash of the concatenation: %l%h%p%r'
  ControlPath ~/.cache/ssh/%C
  ControlPersist yes
  Protocol 2
  Compression yes
  TCPKeepAlive yes
  ServerAliveInterval 60
  ServerAliveCountMax 10

Host athlp0451.local
  User draviolo
  ForwardX11 yes
  IdentityFile ~/Sync/Secret/id_rsa.openbet

Host *.sportsbook.sgdigital.com *.nyxop.net
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null

Host stash.int.openbet.com
  IdentityFile ~/Sync/Secret/id_rsa.openbet

Host tech-ops-jump
  HostName ls3.ps.gameop.net
  User dimrav
  IdentityFile ~/Sync/Secret/id_rsa.openbet

