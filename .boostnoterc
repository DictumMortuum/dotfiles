{
    "amaEnabled": true,
    "editor": {
        "fontFamily": "Hack",
        "fontSize": "14",
        "indentSize": "2",
        "indentType": "space",
        "keyMap": "vim",
        "switchPreview": "BLUR",
        "theme": "default"
    },
    "hotkey": {
        "toggleMain": "Cmd + Alt + L"
    },
    "isSideNavFolded": false,
    "listStyle": "COMPRESSED",
    "listWidth": 200,
    "navWidth": 200,
    "preview": {
        "codeBlockTheme": "default",
        "fontFamily": "Lato",
        "fontSize": "14",
        "lineNumber": true
    },
    "sortBy": {
        "default": "UPDATED_AT"
    },
    "sortTagsBy": "ALPHABETICAL",
    "ui": {
        "defaultNote": "ALWAYS_ASK",
        "disableDirectWrite": false,
        "theme": "white"
    },
    "zoom": 1
}
