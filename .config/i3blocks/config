align=center
separator_block_width=15
markup=none

[keyboard]
command=~/.local/bin/xkb-switch -W
interval=persist

[wired]
label= 
command=~/.dotfiles/bin/iface en
interval=once

[wireless]
label= 
command=~/.dotfiles/bin/iface wlp
interval=once

[time]
command=date "+%Y-%m-%d %H:%M:%S"
interval=1

[hostname]
command=hostname
interval=once
